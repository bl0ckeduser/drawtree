(BLOCK (INT_DECL (DECL_CHILD (VARIABLE:MAX) (NUMBER:100)) (DECL_CHILD (VARIABLE:sieve) (ARRAY_DIM:128)) (DECL_CHILD (VARIABLE:k)) (DECL_CHILD (VARIABLE:n))) (ASGN (ARRAY (VARIABLE:sieve) (NUMBER:1)) (NUMBER:1)) (ASGN (VARIABLE:k) (NUMBER:2)) (WHILE (LT (VARIABLE:k) (VARIABLE:MAX)) (BLOCK (ASGN (ARRAY (VARIABLE:sieve) (POST_INC (VARIABLE:k))) (NUMBER:0)) (CONTLAB))) (ASGN (VARIABLE:k) (NUMBER:1)) (WHILE (LT (POST_INC (VARIABLE:k)) (VARIABLE:MAX)) (BLOCK (BLOCK (ASGN (VARIABLE:n) (NUMBER:2)) (WHILE (LTE (MULT (VARIABLE:n) (VARIABLE:k)) (VARIABLE:MAX)) (BLOCK (BLOCK (ASGN (ARRAY (VARIABLE:sieve) (MULT (VARIABLE:n) (VARIABLE:k))) (NUMBER:1)) (POST_INC (VARIABLE:n))) (CONTLAB)))) (CONTLAB))) (ASGN (VARIABLE:k) (NUMBER:0)) (WHILE (LT (POST_INC (VARIABLE:k)) (VARIABLE:MAX)) (BLOCK (BLOCK (IF (LT (ARRAY (VARIABLE:sieve) (VARIABLE:k)) (NUMBER:1)) (BLOCK (BPF_INSTR:echo (VARIABLE:k))))) (CONTLAB))))
